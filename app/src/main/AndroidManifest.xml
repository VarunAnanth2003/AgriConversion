<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.FLNS.agriconversion">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoleaficon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ConversionActivity"
            android:theme="@android:style/Theme.Material.Light.Dialog.NoActionBar.MinWidth"></activity>
        <activity android:name=".AddNewActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>